{
  "name": "cache-app",
  "version": "1.0.0",
  "description": "A high-performance caching handler using Bun and LRU cache for high-speed and high-load environments.",
  "main": "src/server.ts",
  "private": true,
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "start": "bun run build/src/server.js",
    "dev": "bun run --watch src/server.ts",
    "build": "rimraf ./build && cross-env NODE_ENV=production tsc && tsc-alias && bun build ./ --outdir ./build --target node"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "lru-cache": "^8.0.0",
    "prom-client": "^14.0.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bun": "^1.1.10",
    "@types/express": "^4.17.21",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "cross-env": "^7.0.3",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-html": "^8.1.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^4.1.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.4",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/indrajithc/bun-cache-handler.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/indrajithc/bun-cache-handler/issues"
  },
  "homepage": "https://github.com/indrajithc/bun-cache-handler#readme"
}
